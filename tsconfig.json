{
  "compilerOptions": {
    "incremental": true,
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist/",
    "removeComments": true,
    "importsNotUsedAsValues": "remove",
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true
  }
}
